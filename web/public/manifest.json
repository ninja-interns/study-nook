{
	"manifest_version": 3,
	"name": "Study Nook",
	"version": "0.0.0.1",

	"action": {
		"default_title": "Study Nook",
		"default_popup": "popup.html"
	},
	"description": "Productivity Extension",
	"icons": {
		"16": "logo.png",
		"48": "logo.png",
		"128": "logo.png"
	},

	"host_permissions": ["*://*.studynook.xyz/"],
	"permissions": ["cookies", "storage"]
}
